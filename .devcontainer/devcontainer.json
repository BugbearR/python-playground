{
  "name": "python-playground",
  "dockerComposeFile": [
    "docker-compose.yml"
  ],
  "service": "devcontainer",
  "remoteEnv": {
    "LOCAL_WORKSPACE_FOLDER": "${localWorkspaceFolder}"
  },
  // "remoteUser": "vscode",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "extensions": [
        "EditorConfig.EditorConfig",
        "ms-azuretools.vscode-docker",
        "ms-python.python",
        "ms-toolsai.jupyter"
      ]
    }
  }
}